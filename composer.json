{
    "name": "free-elephants/di",
    "description": "IoC, constructor dependency injection",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "samizdam",
            "email": "samizdam@inbox.ru"
        }
    ],
    "require": {
        "psr/log": "~1.0",
        "psr/container": "^1.0",
        "php": "^7.0|^8.0"
    },
    "provide": {
        "psr/container-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "FreeElephants\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FreeElephants\\DI\\": "tests/",
            "Fixture\\": "tests/Fixture/"
        }
    }
}
